<%= will_paginate %>

<table class="table table-hover">
  <tr>
  	<th>Id</th>
  	<th>Name</th>
  	<th>Difficulty</th>
  </tr>
  <% @questions.each do |question| %>
  <tr>
  	<td><%= question.id %></td>
  	<td><%= link_to question.name, controller: "questions", action: "show", id: question %></td>
  	<td><%= question.difficulty %></td>
  </tr>
  <% end %>
</table>

<%= will_paginate %>